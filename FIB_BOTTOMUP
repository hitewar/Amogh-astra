#include <iostream>
using namespace std;
int fib[5];
//OUR APPROACH IS BOTTOM-UP to caculate fibonacci of a number n 

int fibonacci(int n){
    if(n==0||n==1) return 1;
    fib[0]=1;
    fib[1]=1;
    for(int i=2;i<n;i++)
     fib[i]=fib[i-1]+fib[i-2];
    
    return fib[n-1];   
}

int main() {
   cout<<fibonacci(5);
   for(int i=0;i<5;i++)  cout<<endl<<fib[i];
	// your code goes here
	return 0;
}
